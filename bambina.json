        {
          "foreground": "p:green",
          "style": "plain",
          "template": "<b>()</b>",
          "type": "text"
        },        
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:pink",
          "template": "<p:green><b>  =>  </b></><b>{{ .HEAD }}<p:purple>\u003b</></b>",
          "properties": {
            "fetch_status": true,
            "branch_icon": "<p:purple>Î» </>"
          }
        },
        {
          "foreground": "p:pink",
          "style": "plain",
          "template": "<b> {{if .Version}} ðŸ“¦ {{.Version}} {{end}}</b>",
          "type": "project"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:green",
          "style": "plain",
          "template": "<b> }</b>",
          "type": "text"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
